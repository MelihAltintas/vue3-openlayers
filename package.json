{
  "name": "vue3-openlayers",
  "version": "5.0.0",
  "description": "OpenLayers Wrapper for Vue3",
  "repository": {
    "type": "git",
    "url": "https://github.com/MelihAltintas/vue3-openlayers"
  },
  "homepage": "https://github.com/MelihAltintas/vue3-openlayers",
  "bugs": {
    "url": "https://github.com/MelihAltintas/vue3-openlayers/issues"
  },
  "keywords": [
    "front-end",
    "web",
    "vue",
    "vuejs",
    "vue-js",
    "openlayers",
    "vue3-openlayers",
    "map",
    "gis",
    "vuelayers",
    "vue-openlayers"
  ],
  "author": "Melih Altıntaş",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "./dist/vue3-openlayers.umd.js",
  "module": "./dist/vue3-openlayers.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-openlayers.es.js",
      "require": "./dist/vue3-openlayers.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/vue3-openlayers.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    },
    "./styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "serve": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:dev:debug": "VITE_DEBUG=true npm run docs:dev",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:update-requirements": "node scripts/insert-required-versions.mjs README.md docs/get-started.md",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --write .",
    "prepare": "husky",
    "release": "release-it",
    "release:pre": "npm run release -- --preRelease",
    "release:rc": "npm run release -- --preRelease",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@turf/buffer": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "proj4": "^2.10.0",
    "ol": "^9.0.0",
    "ol-contextmenu": "^5.3.0",
    "ol-ext": "^4.0.15",
    "vue": "3.3.12"
  },
  "peerDependencies": {
    "ol": "^9.0.0",
    "ol-contextmenu": "^5.3.0",
    "ol-ext": "^4.0.15",
    "vue": "^3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.11.25",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^3.2.4",
    "@types/proj4": "^2.5.5",
    "@types/turf": "^3.5.32",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.22.0",
    "get-repository-url": "^2.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1",
    "typescript": "~5.4.2",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "1.0.0-rc.45",
    "vitest": "1.3.1"
  },
  "overrides": {
    "ol-contextmenu": {
      "ol": "^9.x"
    }
  }
}
